/* 可以看到的框框 */
.camera {
  width: 610rpx;
  height: 900rpx;
  border-radius: 50%;
  margin: 20px auto 0;
  position: relative;
}

.camera camera {
  width: 100%;
  height: 100%;
}

button.takePhoto:not([size='mini']) {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100vw;
  height: 90rpx;
  border-radius: 0;
}

